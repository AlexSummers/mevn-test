{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/recommended",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "max-len": ["error", { "code": 250 }],
        "no-console": ["error", { "allow": ["warn", "error"] }]
    }
}